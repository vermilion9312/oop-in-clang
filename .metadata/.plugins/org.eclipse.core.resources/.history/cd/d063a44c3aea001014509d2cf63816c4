#include <clock.h>

static Mode mode = MODE_STOPWATCH;

void operation_clock(void)
{
	switch (mode)
	{
	case CLOCK:
		return;
	case TIMER:
		return;
	case STOPWATCH:
		operate_stopwatch();
		return;
	case ALARM:
		return;
	case MUSIC_PLAYER:
		return;
	default:
		return;
	}
}


void operate_stopwatch(void)
{

}
