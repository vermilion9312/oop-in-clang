#ifndef INC_CLOCK_H_
#define INC_CLOCK_H_

#include <common_include.h>
#include <>

typedef enum {
	CLOCK,
	TIMER,
	STOPWATCH,
	ALARM,
	MUSIC_PLAYER
} Mode;

typedef enum {
	STOPPED,
	PAUSED,
	RUNNING
} State;

#endif /* INC_CLOCK_H_ */
