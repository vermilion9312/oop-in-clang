#include <rgb.h>

void operate_left_rgb(Input* input, Output* output)
{
	if (is_input_rising(input)) toggle_output(output);
}

void operate_right_rgb(Input* input, Output* output)
{
	if (is_input_falling(input)) toggle_output(output);
}
