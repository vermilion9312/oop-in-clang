#ifndef SRC_TIMER_C_
#define SRC_TIMER_C_

#include <timer.h>

static uint32_t time;

void increase_time(void)
{
	time++;
}

uint32_t get_time(TimeIndex index)
{
	switch (index)
	{
	case TINME
	}

	uint32_t seconds = time / 1000;
	return seconds;
}

#endif /* SRC_TIMER_C_ */
